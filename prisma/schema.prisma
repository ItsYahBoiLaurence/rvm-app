generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  messageID String @id
  timestamp String
  userID    String
  sign      String
}

model TransactionData {
  id         String @id @default(cuid())
  rvmID      String
  timestamp  String
  totalCount String
  totalValue String
  item       String
  messageID  String
  userID     String
  sign       String
}
